ฟลักซ์ในพื้นที่ = {RainbowColorValue = 0, HueSelectionPosition = 0}
PresetColor ท้องถิ่น = Color3.fromRGB (66, 134, 255)
Local UserInputService = เกม: GetService ("UserInputService")
local TweenService = เกม: GetService ("TweenService")
Local RunService = เกม: GetService ("RunService")
local LocalPlayer = เกม:GetService("Players")).LocalPlayer
เมาส์ท้องถิ่น = LocalPlayer:GetMouse()
CloseBind ภายในเครื่อง = Enum.KeyCode.RightControl

FluxLib ท้องถิ่น = Instance.new ("ScreenGui")
FluxLib.Name = "FluxLib"
FluxLib.Parent = game.CoreGui
FluxLib.ZIndexBehavior = Enum.ZIndexBehavior.พี่น้อง

coroutine.wrap(
	การทำงาน()
		ในขณะที่รอ() ทำ
			Flux.RainbowColorValue = Flux.RainbowColorValue + 1 / 255
			Flux.HueSelectionPosition = Flux.HueSelectionPosition + 1

			ถ้า Flux.RainbowColorValue >= 1 แล้ว
				Flux.RainbowColorValue = 0
			จบ

			ถ้า Flux.HueSelectionPosition == 80 แล้ว
				Flux.HueSelectionPosition = 0
			จบ
		จบ
	จบ
)()

ฟังก์ชันท้องถิ่น MakeDruggable (topbarobject, วัตถุ)
	การลากในพื้นที่ = ไม่มีเลย
	DragInput ท้องถิ่น = ไม่มี
	DragStart ท้องถิ่น = ไม่มี
	StartPosition ท้องถิ่น = ไม่มี

	ฟังก์ชันท้องถิ่น อัปเดต (อินพุต)
		เดลต้าท้องถิ่น = อินพุตตำแหน่ง - DragStart
		ตำแหน่งท้องถิ่น =
			UDim2.ใหม่(
				StartPosition.X.Scale,
				StartPosition.X.Offset + Delta.X,
				StartPosition.Y.สเกล
				StartPosition.Y.Offset + Delta.Y
			)
		วัตถุตำแหน่ง = pos
	จบ

	topbarobject.InputBegan:เชื่อมต่อ(
		ฟังก์ชั่น (อินพุต)
			ถ้า input.UserInputType == Enum.UserInputType.MouseButton1 หรือ input.UserInputType == Enum.UserInputType.Touch แล้ว
				ลาก = จริง
				DragStart = อินพุต ตำแหน่ง
				StartPosition = วัตถุ ตำแหน่ง

				input.Changed:Connect(
					การทำงาน()
						ถ้า input.UserInputState == Enum.UserInputState.End แล้ว
							การลาก = false
						จบ
					จบ
				)
			จบ
		จบ
	)

	topbarobject.InputChanged:เชื่อมต่อ(
		ฟังก์ชั่น (อินพุต)
			ถ้า
				input.UserInputType == Enum.UserInputType.MouseMovement หรือ
					input.UserInputType == Enum.UserInputType.Touch
			แล้ว
				DragInput = อินพุต
			จบ
		จบ
	)

	UserInputService.InputChanged:เชื่อมต่อ(
		ฟังก์ชั่น (อินพุต)
			ถ้าอินพุต == DragInput และลากแล้ว
				อัปเดต (อินพุต)
			จบ
		จบ
	)
จบ



